<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:context = "http://www.springframework.org/schema/context"
	   xmlns:xsi ="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation = "http://www.springframework.org/schema/beans 
	    http://www.springframework.org/schema/beans/spring-beans.xsd
	    http://www.springframework.org/schema/context
	    http://www.springframework.org/schema/context/spring-context.xsd">
<context:annotation-config />


<bean class="Controller.Activity.ActivityInsertController" /><!-- 활동 등록-->
<bean class="Controller.Activity.ActivityListController" /><!-- 활동 목록 -->
<bean class="Controller.Activity.AcitivtyStoreListForMemberController" /> <!-- 활동등록 상점 목록 -->
<bean class="Controller.Activity.ActivityDetailController" /><!-- 활동 상세보기 -->



<bean class="Controller.Activity.MemberProfileController" /> <!-- 활동 프로필 -->


<!--관리자 -->
<bean class="Controller.ComManagement.ActivityManagementController" />	<!-- 활동 승인/미승인 -->
<bean class="Controller.ComManagement.ActivityStoreController" />	<!-- 활동 상점 등록 -->
<bean class="Controller.ComManagement.ActivityStoreListController" /> <!-- 상점 목록 -->
<bean class="Controller.ComManagement.ActivityProductController" />	<!-- 활동 물품 등록 -->
<bean class="Controller.ComManagement.ActivityStoreDetailController" /><!-- 상점 상세보기 -->
<bean class="Controller.ComManagement.ActivityDeleteStoreController" /> <!-- 상점 삭제 -->







</beans>